package camsreveresefeed;




import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.firefox.FirefoxDriver;
import org.testng.annotations.*;

public class CamsReverseFeedPaytmMoney {
	
	
	
		
	
	public WebDriver driver;
	public DownloadWBR2 downloadDriver;



	

	
	@BeforeMethod

	public  void setUp() {
		String chromedriverPath=System.getProperty("user.dir")+"src/test/resources/driver/chromedriver";
		
		System.setProperty("webdriver.chrome.driver",chromedriverPath);
		driver = new ChromeDriver();
		downloadDriver = new DownloadWBR2(driver);
	}
	
	
	
	





@Test(retryAnalyzer = camsreveresefeed.RetryAnalyzer.class)

public void DSPAlternativeInvestmentFund() throws InterruptedException {

	downloadDriver.downloadWBR2("39 \\ DSP Alternative Investment Fund");
}


@Test(retryAnalyzer = camsreveresefeed.RetryAnalyzer.class)

public void downloadAdityaBirlaSunLifeMutualFund() throws InterruptedException {

	downloadDriver.downloadWBR2("B \\ Aditya Birla Sun Life Mutual Fund");
}






